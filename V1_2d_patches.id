Smpl:=0;
i3:=1;
NPop:=2;
{ Ohm's law }
NP0[1].HH_type:='Lyle';   NP0[1].HH_order:='2-points';
NP0[2].HH_type:='Chow';   NP0[2].HH_order:='2-points';
