//Square[1] :=0.021{s}/0.001{mF/cm^2}/100{MOhms}/1e3; {cm^2}    Square[2]:=Square[1];
//Vreset[1]:=Vrest[1];
//Vreset[2]:=Vreset[1];
//Vrest[2]:=Vrest[1];
NP0[3].Vrest:=-0.065;{V}
//tau_m[2]:=tau_m[1];
NP0[1].ThrShift:=-0.00;{V}
//ThrCorrection[2]:=-0.00;{V}
{ Input}
Qns:=50*0{Hz};
//pext_Iext:=0 {Hz/muA};
dVbipolar_Iext:=3 {V/muA};
T_bipolar:=1;
Iext:=5{muA};
t_end:=0.100;
dt:=0.0001;
Nts:=100;
ts_end:=0.05;
{!!!}
I_ind:=0;//100{pA};
t_ind:=1000{ms};
Form6.CheckBox1.Checked:=false;
{ Neuron properties }
Form4.SpecCond.ItemIndex:=0; {'FixThreshold=10mV'}
sgm_V[1]:=0.002;         sgm_V[2]:=0.002;
{ Blockade }
IfBlockAMPA:=0;   IfBlockGABA:=0;
IfBlockNMDA:=0;   IfBlockGABB:=1;
{ Synapses }
{*****************}
   Form4.gSynC1.Visible:=true;
   If_Binzegger_V1:=0;
{*****************}
gAMP3[1]:=0;               gAMP3[2]:=0;
gAMPA[1]:=0.4;             gAMPA[2]:=3;
gGAB3[1]:=0;               gGAB3[2]:=0;
gGABA[1]:=0.4;             gGABA[2]:=0.4;
gNMD3[1]:=0;
gNMDA[1]:=4*gAMPA[1];
alAMPA[1]:=120; beAMPA[1]:= 600; alAMPA[2]:=120; beAMPA[2]:=600;  VAMPA:= 0.000;
alGABA[1]:= 50; beGABA[1]:= 800; alGABA[2]:= 30; beGABA[2]:=800;  VGABA:=-0.075;
alNMDA[1]:= 10; beNMDA[1]:=150;  alNMDA[2]:= 10; beNMDA[2]:=150;
IfInSyn_al_EQ_beta:=0;
IfSigmoidForPresynRate:=1;
If_m_saturates:=0;  { 04.05.2009 }
factorI:=0;//5.5; {15 if DDSpinEdit90 is Enabled}
factorE:=0;//6;
Form4.DDSpinEdit90.Enabled:=false;
r_axon[1,1]:=0.000100{m}; r_axon[1,2]:=0.000100{m};
r_axon[2,1]:=0.000050{m}; r_axon[2,2]:=0.000050{m};
t_axon[1,1]:=0.0002{s};   t_axon[1,2]:=0.0002{s};
t_axon[2,1]:=0.0002{s};   t_axon[2,2]:=0.0002{s};
{ Clamping }
IfChangeVrest:=1; NP0[3].If_I_V_or_g:=1;
  { 2-D ********************************************************** }
  ni:=35;  nj:=23;
  R_stim:=1;
  ni_stim :=1{35};  nj_stim :=18;
//  ni_stim2:=1; nj_stim2:=18;
  i_view:=12; j_view:=6;
  n_Write:=2;
  WriteOrNot:=1;
  Smpl:=0;
  Form3.DDSpinEdit3.Value:=2; { n_draw }
  Form3.ComboBox2.ItemIndex:=1;   { Integral with exp }
  Form3.CheckBox2.Checked:=false;  { no PinWheels }
  Form3.DDSpinEdit16.Value:=0.8;//99;   { portion of patchy connections }
  { Samples - experimental curves }
  scx_Smpl:=0.001 {s};  scy_Smpl:=1{Hz};
  if KeepParams=0 then begin
     KeepParams:=1;
  end;
end;

