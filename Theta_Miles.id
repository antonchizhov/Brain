procedure CommonParameters;
begin
NSmpls:=1;
{ Ohm's law }
HH_type[1]:='Passive';     HH_order[1]:='2-points';
HH_type[2]:='Chow';        HH_order[2]:='1-point';
{ synaptic currents }
IfInSyn_al_EQ_beta:=0;
{ Input}
Iext:=10 {mkA};
ts:=10 {s};
t_end:=5.000;
dt:=0.00015;
