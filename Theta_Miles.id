{ Noise }
NoiseToSgn:=0.5;
I_ind:=0 {pA};
tauNoise:=0.005 {s};
{ Clamping }
IfChangeVrest:=1; If_I_V_or_p:=2;
  if KeepParams=0 then begin
     Square[1]:=3.72e-5;     Square[2]:=2e-5;
     sgmAP[1]:=0.4;          sgmAP[2]:=0.4;
     gAHP[1]:=0.0; {!}
     gAMP3[1]:=0.2;  gAMP3[2]:=0;
     gAMPA[1]:=2;    gAMPA[2]:=2;
     gGABB[1]:=10;   gGABA[2]:=0;
     alAMPA[1]:=300; beAMPA[1]:=300;
     alAMPA[2]:=300; beAMPA[2]:=300;
     alGABB[1]:=8;   beGABB[1]:=8;
     KeepParams:=1;
  end;
end;

