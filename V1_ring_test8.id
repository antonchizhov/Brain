sgm_V[1]:=0.003;         sgm_V[2]:=0.003;
{ Blockade }
IfBlockAMPA:=0;   IfBlockGABA:=0;
IfBlockNMDA:=0;   IfBlockGABB:=1;
{ Synapses }
{*****************}
   Form4.gSynC1.Visible:=true;
   If_Binzegger_V1:=0;
{*****************}
gAMP3[1]:=0.02{0.03};           gAMP3[2]:=0.02;
gAMPA[1]:=0.05{0.15};           gAMPA[2]:=0.1;
gGABA[1]:=0.3{0.3};             gGABA[2]:=0.2;
